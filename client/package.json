{
  "name": "cricket",
  "displayName": "cricket",
  "description": "An extension that regularly uploads contents of VSC c++ files to a server. To be used to gain insight into how students program.",
  "version": "0.0.1",
  "publisher": "e-UCM",
  "author": {
    "name": "Manuel Freire",
    "e-mail": "manuel.freire@fdi.ucm.es"
  },
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/*.cpp",
    "workspaceContains:**/*.h"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.87.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9"
  },
  "license": "see LICENSE file",
  "repository": {
    "type": "git",
    "url": "https://github.com/manuel-freire/vsc_cricket.git"
  },
  "homepage": "https://github.com/manuel-freire/vsc_cricket"
}
