{
  "name": "cricket",
  "displayName": "cricket",
  "description": "An extension that regularly uploads contents of VSC buffers to a server. To be used to gain insight into how students learn to program.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/*.cpp"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "cricket.showInfo",
      "title": "Hello World2"
    },{
      "command": "cricket.showWarning",
      "title": "Hello World1"
    },{
      "command": "cricket.showSelectionCount",
      "title": "Hello World3"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.87.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9"
  }
}
